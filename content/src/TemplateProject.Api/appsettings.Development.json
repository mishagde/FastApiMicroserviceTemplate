{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "ConnectionStrings": {
    "Postgres": "Host=localhost;Port=5432;Database=templateproject;Username=template;Password=template",
    "Redis": "localhost:6379",
    "RabbitMq": "localhost"
  },

  "Jwt": {
    "Key": "dev-secret-key-123456789"
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "TemplateProject"
    }
  },

  "OpenTelemetry": {
    "OtlpEndpoint": "http://localhost:4318/v1/traces"
  }
}
